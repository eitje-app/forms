{
  "name": "@eitje/form",
  "version": "3.1.24",
  "description": "Beautiful forms made easy",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build:esm": "babel src --out-dir dist/esm --extensions .js,.jsx,.ts,.tsx --env-name esm",
    "build:cjs": "babel src --out-dir dist/cjs --extensions .js,.jsx,.ts,.tsx --env-name cjs",
    "build": "rm -rf dist && npm run build:cjs && npm run build:esm"
  },
  "author": "Abel van Hoek",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eitje-app/forms.git"
  },
  "bugs": {
    "url": "https://github.com/eitje-app/forms/issues"
  },
  "homepage": "https://github.com/eitje-app/forms#readme",
  "peerDependencies": {
    "react": ">=17 <20",
    "react-dom": ">=17 <20"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0"
  },
  "dependencies": {
    "@eitje/utils": "1.0.38",
    "use-context-selector": "^1.4.4",
    "@eitje/web_utils": "1.0.15-alpha",
    "password-validator": "2.2.0"
  }
}
